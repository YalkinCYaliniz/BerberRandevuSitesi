@Model IEnumerable<IdentityUser>
<table class=" table table-sm table-bordered">

<thead>

<tr>

    <th>Id</th>
    <th>Name</th>
    <th>Email</th>
</tr>
</thead>
<tbody>

    @foreach(var user in Model){

        <tr>
            <td>@user.Id</td>
            <td>@user.UserName</td>
            <td>@user.Email</td>

        </tr>

        <td>

            <a href="" class="btn btn-primary btn-sum">Edit</a>
            <a href="" class=" btn btn-danger btn-sum">Delete</a>

        </td>
    }
</tbody>




</table>