<table class=" table table-sm table-bordered">
    <thead>
        <tr>
            <th>Ad</th>
            <th>Soyad</th>
            <th>Kullanıcı Adı</th>
            <th>Email</th>
            <th>Telefon Numarası</th>
            <th>Yaş</th>
            <th>Düzenle</th>
            
        </tr>
    </thead>
    <tbody>
    @foreach(var user in Model){

        <tr>
            <td>@user.ad </td>
            <td>@user.soyad </td>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td>@user.PhoneNumber</td>
            <td>@user.yas</td>  
            <td>
                <a href="" class="btn btn-primary btn-sum">Edit</a>
                <a href="" class=" btn btn-danger btn-sum">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>